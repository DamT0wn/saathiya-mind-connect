{
  "rewrites": [
    {
      "source": "/callback",
      "destination": "/api/auth-handler"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/api/auth/$1"
    },
    {
      "source": "/health",
      "destination": "/api/health"
    },
    {
      "source": "/test",
      "destination": "/api/test"
    }
  ]
}