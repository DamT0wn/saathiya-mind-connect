{
  "functions": {
    "api/oauth-callback.js": {
      "runtime": "@vercel/node@20"
    },
    "api/test.js": {
      "runtime": "@vercel/node@20"
    }
  },
  "rewrites": [
    {
      "source": "/callback",
      "destination": "/api/oauth-callback"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/api/auth/$1"
    },
    {
      "source": "/health",
      "destination": "/api/health"
    },
    {
      "source": "/test",
      "destination": "/api/test"
    }
  ]
}